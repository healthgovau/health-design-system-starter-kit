{
  "name": "hds-starter-kit",
  "version": "1.0.0",
  "description": "Get started fast with the HDS starter kit!",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp sync",
    "cssOnly": "gulp cssOnly",
    "handlebars": "webpack-dev-server --config webpack-handlebars.config.js --mode=development",
    "hb": "webpack --config webpack-handlebars.config.js",
    "html": "webpack-dev-server --config webpack-html.config.js --mode=development",
    "react": "webpack-dev-server --config webpack-react.config.js --mode=development",
    "react:build": "webpack --config webpack-react.config.js --mode=production",
    "setup": "gulp setup"
  },
  "author": "",
  "license": "ISC",
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "json": {
      "enable": false,
      "location": "pancake/",
      "name": "pancake",
      "content": {
        "name": true,
        "version": true,
        "dependencies": true,
        "path": true,
        "settings": true
      }
    },
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "pancake/css/",
      "name": "pancake.min.css"
    },
    "sass": {
      "modules": false,
      "location": "pancake/sass/",
      "name": "pancake.scss"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "pancake/js/",
      "name": "pancake.min.js"
    },
    "react": {
      "location": "pancake/react/"
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@health.gov.au/health-design-system": "github:healthgovau/health-design-system#hds-starterkit-version",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.5",
    "cssnano": "^5.0.8",
    "gulp": "^4.0.2",
    "gulp-compile-handlebars": "^0.6.1",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-sync": "^0.1.4",
    "gulp-terser": "^2.1.0",
    "handlebars-loader": "^1.7.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "sass": "^1.43.3",
    "webpack-dev-server": "^4.3.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.0",
    "file-loader": "^6.2.0",
    "gulp-sourcemaps": "^3.0.0",
    "handlebars-layouts": "^3.1.4",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^4.5.2",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^2.4.5",
    "null-loader": "^4.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^7.0.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "webpack-cli": "^4.9.1",
    "webpack-handlebars-precompiler": "^1.1.0"
  }
}
