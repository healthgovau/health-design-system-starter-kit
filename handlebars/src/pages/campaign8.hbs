    body-class="healthy-pregnancy-campaign"
}}

    {{!-- Subheader --}}
    {{#*inline "subheader-block"}}
        {{> includes/nav--campaigns }}
        {{> includes/subheader--campaign }}
    {{/inline}}

    {{!-- Main content --}}
    {{#*inline "main-block"}}

        {{> includes/band--video }}
        {{# each bands}}
            {{> includes/band
                    band-title=this.band-title
                    band-class=this.band-class
            }}
        {{/each}}

    {{/inline}}

{{/layouts/boilerplate-fullwidth}}


