
{{#> layouts/boilerplate-fullwidth title="Full width page" summary="This is the page summary. Thanks for the memories."}}

    {{!-- Subheader --}}
    {{#*inline "subheader-block"}}
        {{> includes/subheader }}
    {{/inline}}

    {{!-- Main content --}}
    {{#*inline "main-block"}}
        {{# each bands}}
            
